{
  "name": "react-tizen-app",
  "version": "0.1.0",
  "dependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "clean": "yarn install:clean && yarn build:clean && yarn test:clean",
    "install:clean": "rm -rf package-lock.json yarn.lock node_modules",
    "prebuild": "yarn build:clean",
    "build:clean": "rm -rf .buildResult build",
    "build": "react-scripts build",
    "start": "react-scripts start",
    "pretest": "yarn test:clean",
    "test:clean": "rm -rf coverage",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
